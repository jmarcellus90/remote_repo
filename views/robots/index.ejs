<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1>Robots List</h1>
      
      
    <ul>
      <% robots.forEach(function(robot){ %>
        
        <li>
            <a href="/robots/<%= robot._id %>"><%= robot._id %></a>
             <%= robot.name %> - <%= robot.description %>
            

        
        </li>
      <% }); %>
    </ul>
      
      

  </body>
</html>